.tab {
    width: 100%;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    line-height: 80rpx;
    background-color: #fff;
    text-align: center;
    font-size: 30rpx;
    font-weight: 400;
    box-shadow: 0 1px 6px #ccc;
}

.tab-item {
    flex: 1;
    height: 100%;
}

.tab_active {
    color: #009fe9;
    border-bottom: 2px #009fe9 solid;
}

.label {
    width: 140rpx;
    color: rgba(0, 0, 0, 0.3);
    font-size: 30rpx;
    font-weight: 600;
    display: inline-block;
}

.dataList {
    display: flex;
    flex-direction: column;
}

.dataList-tiem {
    height: 300rpx;
    padding: 20rpx;
    display: flex;
    flex-direction: column;
    margin: 10rpx 10rpx 0 10rpx;
    background-color: #fff;
    border-radius: 10rpx;
    box-shadow: 3px 3px 3px rgba(0, 0, 0, 0.3);
}

.dataList-tiem-title {
    height: 50rpx;
    display: flex;
    padding-bottom: 5rpx;
    flex-direction: row;
    justify-content: space-between;
    /* border-bottom: 1rpx solid #c8c7cc; */
    /* margin-bottom: 10rpx; */
}

.tiem-state {
    height: 40rpx;
    padding: 0 20rpx;
    margin-top: -10rpx;
    color: #ccc;
    border: 1px solid #ccc;
}
.state_green{
    color: green;
    border: 1px solid green;
}
.state_red{
    color: red;
    border: 1px solid red;
}
.dataList-tiem-con view{
    height: 50rpx;
}
.dataList-tiem-bot{
    height: 50rpx;
    line-height: 50rpx;
    padding-top: 5rpx;
    border-top: 1rpx solid #c8c7cc;
}
