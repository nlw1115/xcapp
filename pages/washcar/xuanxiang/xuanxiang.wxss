.xuanxiang-container{
    margin-bottom: 150rpx;
}
.title {
    width: 100%;
    height: 60rpx;
    color: grey;
    line-height: 60rpx;
    text-align: center;
}

.options {
    height: 160rpx;
    margin: 10rpx 20rpx;
    padding: 20rpx;
    display: flex;
    flex-direction: row;
    border-radius: 10rpx;
    background-color: #fff;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
}

.icon {
    width: 80rpx;
    height: 160rpx;
    line-height: 160rpx;
    text-align: center;
}

.content {
    flex: 1;
}

.content-head {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.content-head-title {
    font-weight: bold;
}

.content-head-money {
    color: red;
    float: right;
}

.content-dec {
    color: grey;
    font-size: 24rpx;
    height: 96rpx;
    overflow: hidden;
    margin-top: 10rpx;
}
.footer{
    width: 100%;
    height: 90rpx;
    left: 0;
    bottom: 0;
    position: fixed;
    display: flex;
    flex-direction: row;
    background-color: #fff;
    text-align: center;
}
.quxiao{
    width: 320rpx; 
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 10rpx;
    margin: 10rpx 30rpx;
    border: 1px solid rgba(0,0,0,.2);
}
.submit{
    width: 320rpx; 
    height: 70rpx;
    color: #fff;
    line-height: 70rpx;
    border-radius: 10rpx;
    background-color: #009FE9;
    margin: 10rpx 30rpx;
    border: 1px solid rgba(0,0,0,.2);
}


@font-face {
    font-family: "iconfont";
    src: url('iconfont.eot?t=1512308933894'); /* IE9*/
    src: url('iconfont.eot?t=1512308933894#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1512308933894') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1512308933894#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 26px;
    color: gray;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-iconfontoptionbutton:before {
    content: "\e646";
}

.icon-2-copy:before {
    content: "\e54c";
}

.options_active{
    color: #009FE9;
}