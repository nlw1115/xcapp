.car-item {
    width: 100%;
    height: 120rpx;
    display: flex;
    background-color: #fff;
    border-bottom: 1px solid #c8c7cc;
}

.icon {
    width: 120rpx;
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
}

.icon_active {
    color: green;
}

.content {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.addChe{
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    display: flex;
    background-color: #fff;
    flex-direction: row;
    justify-content: space-between;
}
.add-text{
    padding-left: 20rpx;
}
.add-icon{
    width: 120rpx;
    height: 80rpx;
    text-align: center;
}
